{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/Firmware/radio/**",
                "${workspaceFolder}/Firmware/include/**",
                "/usr/share/sdcc/include/**"
            ],
            "defines": [
                "__xdata=",
                "__code=",
                "__pdata=",
                "__data=",
                "__idata=",
                "__bit=uint8_t",
                "at(x)=",
                "__at(x)=",
                "__sbit=volatile bool",
                "__interrupt(x)=",
                "__nonbanked=",
                "__sfr=volatile unsigned char",
                "__sfr16=volatile unsigned int",
                "BOARD_NAME="
            ],
            "compilerPath": "/usr/bin/sdcc",
            "cStandard": "gnu11",
            "cppStandard": "gnu++14",
            "intelliSenseMode": "linux-gcc-x64",
            "configurationProvider": "ms-vscode.makefile-tools"
        }
    ],
    "version": 4
}